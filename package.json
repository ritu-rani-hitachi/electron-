{
  "name": "HitachiBotV1",
  "version": "1.0.2",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js --DEV",
    "test": "node ./app.js --prod",
    "exe": "npm run package-win && npm run win-installer",
    "ubuntu": "npm run package-linux",
    "mac": "npm run package-mac && npm run mac-dmg",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64  --prune=true --out=release-builds",
    "package-win": "electron-packager . HitachiBotV1 --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"HitachiBotV1\"",
    "package-linux": "electron-packager . HitachiBotV1 --overwrite --asar=true --platform=linux --arch=x64  --prune=true --out=release-builds",
    "win-installer": "node build.js --platform=window",
    "mac-dmg": "node build.js --platform=mac"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Hitachi",
  "manufacturer": "Hitachi",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-installer-dmg": "^3.0.0",
    "minimist": "^1.2.0"
  },
  "dependencies": {
    "base64topdf": "^1.1.8",
    "electron-packager": "^12.1.0",
    "electron-winstaller": "^2.6.4",
    "node-notifier": "^5.2.1"
  }
}
